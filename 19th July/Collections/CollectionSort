package com.collections;

import java.util.*;

public class CollectionsSort {

	public static void main(String[] args) {
	List<String> l=Arrays.asList("ben","ken","den");
	Collections.sort(l);
	System.out.println(l);
	List<Integer> l1=Arrays.asList(23,12,34,25);
	Collections.sort(l1);
	System.out.println(l1);
	}

}
